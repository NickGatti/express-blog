{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 Chrome versions",
            "last 2 Firefox versions",
            "last 2 Edge versions",
            "last 2 iOS versions"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": ["syntax-dynamic-import", "transform-object-rest-spread"],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node", "transform-object-rest-spread"]
    }
  }
}